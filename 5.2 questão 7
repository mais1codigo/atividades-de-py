


paridade = lambda x: 'par' if x % 2 == 0 else 'ímpar'

def main():
    print()
    while True:
        try:
            valor = int(input())
            if valor == 0:
                break
            print(paridade(valor))
        except ValueError:
            print("Por favor, digite um número inteiro válido.")

if __name__ == "__main__":
    main()
