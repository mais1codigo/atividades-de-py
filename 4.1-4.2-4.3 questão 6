maior = menor = None

while True:
    valor = int(input("Digite um valor (0 para encerrar): "))
    
    if valor == 0:
        break
    
    if maior is None or valor > maior:
        maior = valor
    if menor is None or valor < menor:
        menor = valor

print(f"Maior: {maior}")
print(f"Menor: {menor}")


